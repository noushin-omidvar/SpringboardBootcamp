{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "required": [
        "title",
        "salary",
        "equity",
        "company_handle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1,
            "title": "The title Schema",
            "examples": [
                "ML engineer"
            ]
        },
        "id": {
            "type": "integer",
            "default": 1,
            "title": "The id Schema",
            "examples": [
                1
            ]
        },
        "salary": {
            "type": "number",
            "default": 0.0,
            "title": "The salary Schema",
            "examples": [
                125000
            ]
        },
        "equity": {
            "type": "number",
            "default": 0.0,
            "minimum":0,
            "maximum":1,
            "title": "The equity Schema",
            "examples": [
                0.5
            ]
        },
        "company_handle": {
            "type": "string",
            "minLength": 1,
            "maxLength": 25,
            "title": "The company_handle Schema",
            "examples": [
                "apple"
            ]
        }
    },
    "examples": [{
        "title": "ML engineer",
        "id": 1,
        "salary": 125000,
        "equity": 0.5,
        "company_handle": "apple"
    }]
}